{
  "version": "0.2",
  "language": "ru,en",
  "dictionaries": [
    "ru_ru",
    "en_us",
    "latex"
  ],
  "import": [
    "@cspell/dict-ru_ru/cspell-ext.json",
    "@cspell/dict-en_us/cspell-ext.json"
  ],
  "ignorePaths": [
    "node_modules",
    ".git",
    "*.pdf",
    "*.aux",
    "*.log",
    "*.out",
    "*.toc",
    "*.bbl",
    "*.blg"
  ],
  "words": [
    "Actions",
    "API",
    "CI/CD",
    "GitHub",
    "JSON",
    "LaTeX",
    "PDF",
    "README",
    "workflow",
    "YAML",
    "Makeatletter",
    "микротипографики",
    "оркестрацию",
    "ugost"
  ],
  "ignoreWords": [],
  "useGitignore": true,
  "enableFiletypes": [
    "latex",
    "tex"
  ],
  "allowCompoundWords": true,
  "caseSensitive": false,
  "flagWords": []
}
