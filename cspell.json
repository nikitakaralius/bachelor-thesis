{
  "version": "0.2",
  "language": "ru,en",
  "dictionaries": [
    "ru_RU",
    "en_US",
    "latex",
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "./.spellcheckignore",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "node_modules",
    ".git",
    "*.pdf",
    "*.aux",
    "*.log",
    "*.out",
    "*.toc",
    "*.bbl",
    "*.blg"
  ],
  "words": [],
  "ignoreWords": [],
  "useGitignore": true,
  "enableFiletypes": [
    "latex",
    "tex"
  ]
}
