{
  "version": "0.2",
  "language": "ru,en",
  "dictionaries": [
    "ru_ru",
    "en_us",
    "latex",
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "./.spellcheckignore",
      "addWords": true
    }
  ],
  "import": [
    "@cspell/dict-ru_ru/cspell-ext.json",
    "@cspell/dict-en_us/cspell-ext.json"
  ],
  "ignorePaths": [
    "node_modules",
    ".git",
    "*.pdf",
    "*.aux",
    "*.log",
    "*.out",
    "*.toc",
    "*.bbl",
    "*.blg"
  ],
  "words": [],
  "ignoreWords": [],
  "useGitignore": true,
  "enableFiletypes": [
    "latex",
    "tex"
  ],
  "allowCompoundWords": true,
  "caseSensitive": false,
  "flagWords": []
}
